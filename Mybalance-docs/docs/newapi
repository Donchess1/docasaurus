    {
      "id": "497f6eca-6276-4993-bfeb-53cbbbba6f08",
      "status": "PENDING",
      "type": "DEPOSIT",
      "mode": "string",
      "reference": "string",
      "amount": 0,
      "charge": 0,
      "currency": "NGN",
      "meta": null,
      "customer_role": "string",
      "verified": true,
      "escrow": "string",
      "created_at": "2019-08-24T14:15:22Z",
      "updated_at": "2019-08-24T14:15:22Z"
    }
  ]
